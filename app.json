{
  "expo": {
    "name": "Quickbites",
    "slug": "quickbites",
    "scheme": "com.srahin000.quickbites",
    "deepLinkScheme": "com.srahin000.quickbites",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/image.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/image.png",
          "color": "#8B5CF6",
          "mode": "production"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "otherXcodeBuildSettings": {
              "ENABLE_USER_SCRIPT_SANDBOXING": "NO"
            }
          }
        }
      ]
    ],
    "notification": {
      "icon": "./assets/image.png",
      "color": "#8B5CF6",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "splash": {
      "image": "./assets/Untitled design (1).png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.srahin000.quickbites",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsLocalNetworking": true,
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            }
          }
        }
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.com.qbdelivery.quickbitesdelivery"
        ]
      },
      "associatedDomains": [
        "applinks:quickbites-delta.vercel.app"
      ]
    },
    "android": {
      "package": "com.srahin000.quickbites",
      "adaptiveIcon": {
        "foregroundImage": "./assets/image.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "quickbites-delta.vercel.app"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/image.png"
    },
    "extra": {
      "eas": {
        "projectId": "444ed843-9ec5-4779-8b4a-45817fd22e20"
      }
    },
    "runtimeVersion": "1.0.2",
    "updates": {
      "url": "https://u.expo.dev/444ed843-9ec5-4779-8b4a-45817fd22e20"
    }
  }
}
